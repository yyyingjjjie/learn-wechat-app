/* pages/index/index.wxss */
.image-header{
  height: 100px;
  width: 100%;
}
.api-content{
  padding: 16px;
}
icon{
  vertical-align: bottom;
  margin-right: 10px;
}
.parent-item{
  margin-bottom: 10px;
}
.parent-item text{
  color: #333;
  font-size: 14px;
  line-height: 20px;
  font-weight: bold;
}
.child-wrap{
  overflow: hidden;
}
.child-item{
  float: left;
  width: calc(50% - 32px);
  text-align: left;
  padding: 5px 16px;
  font-size: 12px;
  color: #0a83ff;
  cursor: pointer;
}